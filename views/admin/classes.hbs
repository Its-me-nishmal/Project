              <script>

document.addEventListener('DOMContentLoaded', function() {
  // Get all elements with the class 'k'
  const tdElements = document.querySelectorAll('.k');

  // Iterate through the NodeList and set the content
  tdElements.forEach((tdElement) => {
    try {
      const playlistUrl = tdElement.textContent.trim();
      const playlistId = (new URL(playlistUrl).searchParams.get('list')) || playlistUrl;
      tdElement.innerText = playlistId;
    } catch (error) {
      console.error(`Error processing URL: ${tdElement.textContent.trim()}`, error.message);
      // Handle the error gracefully, e.g., set a default value or skip the element
    }
  });
});

